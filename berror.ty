function
  prueba(matrix(2,2) a, number b)
  return matrix(2,2)
  begin
     return a .*. b;
  end;

function
  scale(number s, row(3) r)
  return row(3)
  begin
     return r .*. s;
  end;
function
  doubleit(number s)
  return number
  begin
    return s*2;
 end;
